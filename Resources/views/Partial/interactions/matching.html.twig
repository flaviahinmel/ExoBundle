<a id="displayspecificFeatures" class="btn btn" data-toggle="collapse" data-target="#specificFeatures"
   title="{{ "display hide parameters" | trans }}">
    <i class="fa fa-edit"></i> {{ 'matching parameters' | trans }}
</a><hr>

<div id="specificFeatures" class="list-group collapse">
    <div id="Instructions" class="alert alert-info">
        <a class="close" onclick="HideInstruction();">&times;</a>
        <p>{{ 'instruction matching' | trans }}</p>
    </div>
    </br>
    <a class="btn btn-default" onclick="DisplayInstruction();" id="showInstruct">
        <i class="fa fa-question"></i> {{ 'show Instructions' | trans }}
    </a>
    <a class="btn btn-default" onclick="HideInstruction();" id="hideInstruct">
        <i class="fa fa-question"></i> {{ 'hide Instructions' | trans }}
    </a>
    </br></br>
    <div>
        {{ form_row(form.typeMatching) }}
            <table>
                <tr>
                    <td>
                        <div id="proposalError"></div>
                        <div id="tableProposal" style='text-align: center;'></div>
                        {{ form_row(form.proposals) | replace({"col-md-3": "col-md-12"}) | raw }}
                        {{ form_errors(form.proposals) }}
                    </td>
                    <td style='padding: 20px;'> </td>
                    <td>
                        <div id="labelError"></div>
                        <div id="tableLabel" style='text-align: center;'></div>
                        {{ form_row(form.labels) | replace({"col-md-3": "col-md-12"}) | raw }}
                        {{ form_errors(form.labels) }}
                    </td>
                </tr>
            </table>
    </div>
    <a class="btn btn col-md-offset-10" data-toggle="collapse" data-target="#specificFeatures"
        title="{{ "display hide parameters" | trans }}" >
         <i class="fa fa-eye-slash"></i> {{ 'mask' | trans }}
    </a>
    <hr>
</div>